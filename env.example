# AWS Deployment Environment Configuration
# Copy this file to .env and fill in your actual values

# JWT Secret (generate a strong secret - at least 64 characters)
JWT_SECRET=your-super-secret-jwt-key-here-make-it-at-least-64-characters-long

# EC2 Public IP (replace with your actual EC2 public IP)
EC2_PUBLIC_IP=your-ec2-public-ip-here

# Database URLs (replace with your RDS endpoint)
USER_DB_URL=jdbc:postgresql://your-rds-endpoint:5432/ijaa_users?sslmode=require
EVENT_DB_URL=jdbc:postgresql://your-rds-endpoint:5432/ijaa_events?sslmode=require
FILE_DB_URL=jdbc:postgresql://your-rds-endpoint:5432/ijaa_files?sslmode=require

# Database Credentials (replace with your actual database credentials)
USER_DB_USERNAME=ijaa_user
USER_DB_PASSWORD=your_user_password_here
EVENT_DB_USERNAME=ijaa_event
EVENT_DB_PASSWORD=your_event_password_here
FILE_DB_USERNAME=ijaa_file
FILE_DB_PASSWORD=your_file_password_here

# Optional: Custom server URLs (if you want to override the defaults)
# PRODUCTION_SERVER_URL=http://your-domain.com
# GATEWAY_SERVER_URL=http://your-domain.com
